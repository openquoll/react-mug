{
  "name": "react-mug",
  "version": "0.0.1",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "build-cjs": "tsc -p ./builder/tsconfig.cjs.json",
    "prebuild-cjs": "shx rm -fr ./builder/cache/",
    "build-esm": "tsc -p ./builder/tsconfig.esm.json",
    "prebuild-esm": "shx rm -fr ./builder/cache/",
    "clean-dist": "shx rm -fr ./dist/",
    "clean-deps": "shx rm -fr ./package-lock.json ./node_modules/",
    "test": "run-s main-test type-test",
    "main-test": "jest",
    "type-test": "tsd -t . -f \"./@(src|tests)/**/*.test-d.ts?(x)\"",
    "type-test-watch": "chokidar \"./src/**/*.ts?(x)\" \"./tests/**/*.test-d.ts?(x)\" --command \"npm run type-test\" --initial"
  },
  "devDependencies": {
    "@swc/core": "~1.7.11",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/react": "~16.0.0",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/copy-paste": "~1.1.33",
    "@types/jest": "~29.5.12",
    "@types/react": "~18.3.3",
    "@types/react-dom": "~18.3.0",
    "chokidar-cli": "~3.0.0",
    "copy-paste": "~1.5.3",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shx": "~0.3.4",
    "ts-jest": "~29.1.4",
    "ts-node": "~10.9.2",
    "tsd": "~0.31.1",
    "typescript": "~5.4.5"
  }
}
